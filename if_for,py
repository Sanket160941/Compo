a = 10
b = 15

if a>b:
    print("A is greater than B")
else:
    print("B is greater than A")

print(a>b)
print(a<b)
print(a>=b)
print(a<=b)
print(a==b)
print(a!=b)


a = 35
b = 20

if a>b:
    print("A is greater than B")
elif a<b:
    print("B is greater than A")
else:
    print("A is equal than B")

